<html>
<head>
    <meta http-equiv="Content-Type" content="text/html;charset=utf-8">
    <title>History</title>
</head>
<body>
    <h1>History</h1>

    <p style="color: red;">
        {{ errors }}
    </p>

    {% if form.errors %}
        <p style="color: red;">
            Please correct the error{{ form.errors|pluralize }} below.
        </p>
    {% endif %}

    <form action="" method="post">
        <table>
            {{ form.as_table }}
        </table>
        <input type="submit" value="发送">
    </form>
</body>
</html>
